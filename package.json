    "tabby",
    "tabby-plugin",
    "terminal",
    "hotkey",
    "iflow",
    "newline"
  ],
  "author": "ABen",
  "license": "ISC",
  "type": "commonjs",
  "peerDependencies": {
    "@angular/core": "^15.0.0",
    "@angular/common": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "tabby-core": "^1.0.197-nightly.1",
    "tabby-terminal": "^1.0.197-nightly.1",
    "tabby-settings": "^1.0.197-nightly.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben91lin/tabby-clear.git"
  },
  "bugs": {
    "url": "https://github.com/ben91lin/tabby-clear/issues"
  },
  "homepage": "https://github.com/ben91lin/tabby-clear#readme",
